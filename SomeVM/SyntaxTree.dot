digraph SomeASM
{
    start -> 
    {
        comment;
        newline;
        instruction;
        EOF;
    }
    
    comment -> newline;
    newline -> start;
    semicolon -> start;
    EOF -> stop;
    
    instruction ->
    {
        newline;
        variable;
        literal;
        semicolon;
    }
        
    variable -> 
    {
        variable;
        literal;
        semicolon;
    }
    
    
    literal -> 
    {
        literal;
        variable;
        semicolon;
    }
}
